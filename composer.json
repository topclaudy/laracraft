{
  "name": "awobaz/laracraft",
  "description": "Theming system for Laravel application",
  "license": "MIT",
  "keywords": [
    "laravel",
    "laravel blade",
    "laravel layout",
    "laravel theme"
  ],
  "authors": [
    {
      "name": "Claudin J. Daniel",
      "email": "cdaniel@awobaz.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "laravel/framework": "~5.7",
    "doctrine/dbal": "^2.5",
    "symfony/var-dumper": "^4.1",
    "spatie/laravel-sluggable": "^2.1",
    "thecodingmachine/class-explorer": "^1.0",
    "nothingworks/blade-svg": "^0.3.1",
    "jenssegers/model": "^1.2",
    "tightenco/ziggy": "^0.6.9"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "fzaninotto/faker": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Awobaz\\Laracraft\\": "src"
    },
    "files": [
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Awobaz\\Laracraft\\LaracraftServiceProvider"
      ]
    }
  },
  "suggest": {
    "awobaz/compoships": "Multi-columns relationships for Laravel 5's Eloquent",
    "awobaz/eloquent-auto-append": "Automatically append accessors to model serialization",
    "awobaz/blade-active": "Blade directives for the Laravel 'Active' package",
    "awobaz/syntactic": "Syntactic sugar for named and indexed parameters call."
  }
}
